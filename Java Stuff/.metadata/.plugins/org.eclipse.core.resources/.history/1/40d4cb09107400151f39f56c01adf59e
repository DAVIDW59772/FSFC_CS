/**
 * @author CompUser
 *
 */
public class IfandElseLoops {
	
	public static void main(String args[]){
		int x = 30;
		
		if(x > 10){
			System.out.print("Value of X is less than 10");
		}else if(10 < x && x < 20){
			System.out.print("Value of X is less than 20");
		}else{
			System.out.print("X is not less than 20");
		}
		}
}
