
public class TestImmutableString {
	public static void main(String args[]){
		String titleOne = "Operation Flashpoint";
		String titleTwo = "Cold War Assualt";
		String titleThree = "Operation Flashpoint";
		String titleFour = new String("Cold War Assualt");
		
		System.out.println(titleOne.equals(titleThree));
		
		System.out.println(titleOne == titleThree);
		
		System.out.println(titleTwo == titleFour);
	}
}
